         :%>ӹ    NMN_OBLIQUE-INDEFINITE-SG_FRONT ;
!        :%^А2ЫӸ NMN_OBLIQUE-INDEFINITE-SG_FRONT ;
         :%>ӹ    NMN_OBLIQUE-POSSESSA-SG_FRONT   ;
!        :%^А2ЫӸ NMN_OBLIQUE-POSSESSA-SG_FRONT   ;

LEXICON NMN_VAREN1E-PL
 :%>ӹ NMN_OBLIQUE-INDEFINITE-PL_MUTUAL ; !!= * __@CODE@__ %>влӓ
 :%>ӹ NMN_OBLIQUE-POSSESSA-PL_FRONT    ;

LEXICON NMN_OLMA !!= @CODE@  олма:олма
!! Back harmony
  NMN_OLMA-SG ;
  NMN_OLMA-PL ;
LEXICON NMN_OLMA-SG
        +Sg+Nom:                    K                              ;
                                    NMN_OBLIQUE-INDEFINITE-SG_BACK ;
                                    NMN_OBLIQUE-POSSESSA-SG_BACK   ;
