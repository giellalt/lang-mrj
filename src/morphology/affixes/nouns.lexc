!! !!!Noun inflection
!! ----

!! Hill Mari nouns inflect in cases.
LEXICON NTag
  N_ ;

! SNM: ensure there is a space before the jspwiki markup __ when the preceding
! character (ie last of lexicon name) is also an _ char - otherwise one gets a
! jspwiki syntax error.
LEXICON N_  !!≈ * __@CODE@ __
 +N+WORK: K ;

!! !!Substandard

LEXICON N_KOL_SUB !!≈ * __@CODE@__
 +Err/Orth: N_KOL ;

LEXICON N_TYERVYE_SUB !!≈ * __@CODE@__
 +Err/Orth: N_TYERVYE ;

!! !!Standard

LEXICON N_KOL !!= @CODE@ кол:кол
!! Back harmony
  N_KOL-SG ;
  N_KOL-PL ;
LEXICON N_KOL-SG
 +N:%{back%} NMN_KOL-SG ;
LEXICON N_KOL-PL
 +N:%{back%} NMN_KOL-PL ;

LEXICON N_MOER !!= @CODE@ мӧр:мӧр
!! Front harmony
  N_MOER-SG ;
  N_MOER-PL ;
LEXICON N_MOER-SG
 +N:%{front%} NMN_MOER-SG ;
LEXICON N_MOER-PL
 +N:%{front%} NMN_MOER-PL ;

LEXICON N_MARY !!≈ * __@CODE@__ мары:мары
!! The stem vowel "е" is found with possessor indices and the lative
!! Back harmony
  N_MARY-SG ;
  N_MARY-PL ;
LEXICON N_MARY-SG
 +N:%{back%} NMN_MARY-SG ;
LEXICON N_MARY-PL
 +N:%{back%} NMN_MARY-PL ;

LEXICON N_TYERVYE !!≈ * __@CODE@__ тӹрвӹ:тӹрвӹ
!! The stem vowel "е" is found with possessor indices and the lative
!! Front harmony
  N_TYERVYE-SG ;
  N_TYERVYE-PL ;
LEXICON N_TYERVYE-SG
 +N:%{front%} NMN_TYERVYE-SG ;
LEXICON N_TYERVYE-PL
 +N:%{front%} NMN_TYERVYE-PL ;

LEXICON N_KARTA_SUB
 +Err/Orth: N_KARTA ;

LEXICON N_KARTA !!≈ * __@CODE@__ карта:карта
!! а is specific to the +Sg+Nom, elsewhere ы or ӹ
!! Back harmony
  N_KARTA-SG ;
  N_KARTA-PL ;
LEXICON N_KARTA-SG
 +N:%{back%} NMN_KARTA-SG ;
LEXICON N_KARTA-PL
 +N:%{back%} NMN_KARTA-PL ;

LEXICON N_BIRZHA !!≈ * __@CODE@__ биржа:биржа
!! а is specific to the +Sg+Nom, elsewhere  ӹ
!! Front harmony
  N_BIRZHA-SG ;
  N_BIRZHA-PL ;
LEXICON N_BIRZHA-SG
 +N:%{front%} NMN_BIRZHA-SG ;
LEXICON N_BIRZHA-PL
 +N:%{front%} NMN_BIRZHA-PL ;

LEXICON N_VAREN1E !!≈ * __@CODE@__ варенье:варенье
!! е is specific to the +Sg+Nom, elsewhere  ӹ
!! Front harmony
  N_VAREN1E-SG ;
  N_VAREN1E-PL ;
LEXICON N_VAREN1E-SG
 +N:%{front%} NMN_VAREN1E-SG ;
LEXICON N_VAREN1E-PL
 +N:%{front%} NMN_VAREN1E-PL ;

LEXICON N_POCHTA !!= @CODE@  почта:почта
        +N:%{back%} NMN_OBLIQUE-INDEFINITE-SG_BACK   ;
        +N:%{back%} NMN_OBLIQUE-INDEFINITE-PL_MUTUAL ;
        +N:%{back%} NMN_OBLIQUE-POSSESSA-SG_BACK     ; !!!!
        +N:%{back%} NMN_OBLIQUE-POSSESSA-PL_BACK     ;
 +N+Sg+Nom:%{back%} K                                ;

LEXICON N_OLMA_SUB
 +Err/Orth: N_OLMA ;
LEXICON N_OLMA !!= @CODE@  олма:олма
!! Back harmony
  N_OLMA-SG ;
  N_OLMA-PL ;
LEXICON N_OLMA-SG
 +N:%{back%} NMN_OLMA-SG ;
LEXICON N_OLMA-PL
 +N:%{back%} NMN_OLMA-PL ;

LEXICON N_AEZAE !!= @CODE@  ӓзӓ:ӓзӓ
!! Front harmony
  N_AEZAE-SG ;
  N_AEZAE-PL ;
LEXICON N_AEZAE-SG
 +N:%{front%} NMN_AEZAE-SG ;
LEXICON N_AEZAE-PL
 +N:%{front%} NMN_AEZAE-PL ;

LEXICON N_PECHEN1E !!= @CODE@  печенье:печенье
!! Front harmony
  N_PECHEN1E-SG ;
  N_PECHEN1E-PL ;
LEXICON N_PECHEN1E-SG
 +N:%{front%} NMN_PECHEN1E-SG ;
LEXICON N_PECHEN1E-PL
 +N:%{front%} NMN_PECHEN1E-PL ;

!!
LEXICON NMN_KOL !!= @CODE@ кол:кол
!! Back harmony
