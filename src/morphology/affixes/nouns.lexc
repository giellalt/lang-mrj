          NMN_OBLIQUE-INDEFINITE-SG_BACK ;
          NMN_OBLIQUE-POSSESSA-SG_BACK   ;

LEXICON NMN_KOL-PL
  NMN_OBLIQUE-INDEFINITE-PL_MUTUAL ;
  NMN_OBLIQUE-POSSESSA-PL_BACK     ;

LEXICON NMN_MOER !!= @CODE@ мӧр:мӧр
