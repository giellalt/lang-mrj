+N+PxSg1+Pl+Nom:эмвлӓ K ;
+N+Pl+Nom+PxSg1:влӓэм K ;
+N+PxSg2+Sg+Nom:эт K ;
+N+PxSg2+Pl+Nom:этвлӓ K ;
+N+Pl+Nom+PxSg2:влӓэт K ;
+N+Sg+Lat+PxSg3:%>эшЫ1жЫ1	   K  ;

LEXICON NMN_OBLIQUE-INDEFINITE
+Sg+Gen:%>Ы1н K ; ! 
+Sg+Acc:%>Ы1м K ; !
+Sg+Com:%>ге K ; ! (with a village)
+Sg+Dat:%>лА1н K ; !
+Sg+Ine:%>Ы1штЫ1 K ; ! (in a village)
+Sg+Ill:%>Ы1шкЫ1 K ; ! (to a village)
+Sg+Ill:%>Ы1ш K ; ! (to a village)
+Sg+Lat:%>Е2ш K ; ! (into a village)
+Sg+Comp:%>лА1 K ; ! (like a village)
+Sg+Abe:%>де K ; ! (without a village)
!!
+Pl+Nom:влӓ 	  K	; 
+Pl+Gen:влӓ%>Ы1н K ; ! 
+Pl+Acc:влӓ%>Ы1м K ; !
+Pl+Com:влӓ%>ге K ; ! (with a village)
+Pl+Dat:влӓ%>лА1н K ; !
+Pl+Ine:влӓ%>Ы1штЫ1 K ; ! (in a village)
+Pl+Ill:влӓ%>Ы1шкЫ1 K ; ! (to a village)
+Pl+Ill:влӓ%>Ы1ш K ; ! (to a village)
+Pl+Lat:влӓ%>Е2ш K ; ! (into a village) 
+Pl+Comp:влӓ%>лА1 K ; ! (like a village)
+Pl+Abe:влӓ%>де K ; ! (without a village)

LEXICON N_01
+Sg+Gen:%>Ы1н K ; ! 
