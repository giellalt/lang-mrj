  NMN_MARY-PL ;
LEXICON NMN_MARY-SG
 +Sg+Nom: K                              ;
          NMN_OBLIQUE-INDEFINITE-SG_BACK ;
          NMN_OBLIQUE-POSSESSA-SG_BACK   ;

LEXICON NMN_MARY-PL
            NMN_OBLIQUE-INDEFINITE-PL_MUTUAL      ;
            NMN_OBLIQUE-POSSESSA-PL_BACK          ;
! +PxSg1:эм PL-ABEACCCOMDATGENILLINELATNOM_MUTUAL ;

LEXICON NMN_TYERVYE !!≈ * __@CODE@__ тӹрвӹ:тӹрвӹ
!! The stem vowel "е" is found with possessor indices and the lative
!! Front harmony
  NMN_TYERVYE-SG ;
  NMN_TYERVYE-PL ;
LEXICON NMN_TYERVYE-SG
 +Sg+Nom: K                               ;
          NMN_OBLIQUE-INDEFINITE-SG_FRONT ;
          NMN_OBLIQUE-POSSESSA-SG_FRONT   ;

LEXICON NMN_TYERVYE-PL
            NMN_OBLIQUE-INDEFINITE-PL_MUTUAL      ;
            NMN_OBLIQUE-POSSESSA-PL_FRONT         ;
! +PxSg1:эм PL-ABEACCCOMDATGENILLINELATNOM_MUTUAL ;

LEXICON NMN_KARTA	 !!≈ * __@CODE@__ карта:карта
!! Back а/ы
!! а is specific to the +Sg+Nom, elsewhere ы
  NMN_KARTA-SG ;
  NMN_KARTA-PL ;
LEXICON NMN_KARTA-SG
 +Sg+Nom:    K                              ;
        :%>ы NMN_OBLIQUE-INDEFINITE-SG_BACK ;
        :%>ы NMN_OBLIQUE-POSSESSA-SG_BACK   ;

LEXICON NMN_KARTA-PL
 :%>ы NMN_OBLIQUE-INDEFINITE-PL_MUTUAL ;
 :%>ы NMN_OBLIQUE-POSSESSA-PL_BACK     ;

LEXICON NMN_BIRZHA	 !!≈ * __@CODE@__ биржа:биржа
!! Front а/ӹ
!! а is specific to the +Sg+Nom, elsewhere ӹ
