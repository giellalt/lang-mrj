7:шӹмлӹ LÅGEVT ;
8:кӓндӓкшлӹ LÅGEVT ;
9:ӹндекшлӹ LÅGEVT ;

LEXICON LÅGEVT
%0:  THOUSANDS ;
 ONEST ;

LEXICON ONEST
