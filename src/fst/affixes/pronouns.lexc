!! # Pronouns
!! ----

!! Hill Mari (Western Mari) pronouns...

LEXICON PRON_
 : WORDEND ;

LEXICON PERS-SG1
  +Pers+Sg1+Nom:нь    K ;
  +Pers+Sg1+Gen:ньӹн  K ;
  +Pers+Sg1+Acc:ньӹм  K ;
  +Pers+Sg1+Dat:лӓнем K ;
 +Pers+Sg1+Cmpr:ньлӓ  K ;
  +Pers+Sg1+Com:ньге  K ;

LEXICON PERS-SG2
  +Pers+Sg2+Nom:нь    K ;
  +Pers+Sg2+Gen:ньӹн  K ;
  +Pers+Sg2+Acc:ньӹм  K ;
  +Pers+Sg2+Dat:лӓнет K ;
 +Pers+Sg2+Cmpr:ньлӓ  K ;
  +Pers+Sg2+Com:ньге  K ;

LEXICON PERS-SG3
  +Pers+Sg3+Nom:      K ;
  +Pers+Sg3+Gen:%>н   K ;
  +Pers+Sg3+Acc:%>м   K ;
  +Pers+Sg3+Dat:%>лӓн K ;
 +Pers+Sg3+Cmpr:%>лӓ  K ;
  +Pers+Sg3+Com:%>ге  K ;

LEXICON PERS-PL1
  +Pers+Pl1+Nom:        K ;
  +Pers+Pl1+Gen:мнӓ%>н  K ;
  +Pers+Pl1+Acc:мнӓ%>м  K ;
  +Pers+Pl1+Dat:%>лӓннӓ K ;
 +Pers+Pl1+Cmpr:%>лӓ    K ;
  +Pers+Pl1+Com:%>ге    K ;

LEXICON PERS-PL2
  +Pers+Pl2+Nom:        K ;
  +Pers+Pl2+Gen:мдӓ%>н  K ;
  +Pers+Pl2+Acc:мдӓ%>м  K ;
  +Pers+Pl2+Dat:%>лӓндӓ K ;
 +Pers+Pl2+Cmpr:%>лӓ    K ;
  +Pers+Pl2+Com:%>ге    K ;

LEXICON PERS-PL3 !!= @CODE@ нӹнӹ:нӹнӹ
  +Pers+Pl3+Nom:      K ;
  +Pers+Pl3+Gen:%>н   K ;
  +Pers+Pl3+Acc:%>м   K ;
  +Pers+Pl3+Dat:%>лӓн K ;
 +Pers+Pl3+Cmpr:%>лӓ  K ;
  +Pers+Pl3+Com:%>ге  K ;

LEXICON REFL !!= @CODE@ ӹшке:ӹшк
      +Refl+Dep:е          K ;
      +Refl+Gen:ӹмӹ%>н     K ;
      +Refl+Acc:ӹмӹ%>м     K ;
      +Refl+Dat:ӹ%>лӓн     K ;

! +Refl+Sg1+Nom:е          K ;
  +Refl+Sg1+Gen:ӹмемӹ%>н   K ;
  +Refl+Sg1+Acc:ӹмемӹ%>м   K ;
  +Refl+Sg1+Dat:ӹ%>лӓнем   K ;

! +Refl+Sg2+Nom:е          K ;
  +Refl+Sg2+Gen:ӹметӹ%>н   K ;
  +Refl+Sg2+Acc:ӹметӹ%>м   K ;
  +Refl+Sg2+Dat:ӹ%>лӓнет   K ;

! +Refl+Sg3+Nom:е          K ;
  +Refl+Sg3+Gen:ӹмжӹ%>н    K ;
  +Refl+Sg3+Acc:ӹмжӹ%>м    K ;
  +Refl+Sg3+Dat:ӹ%>лӓнжӹ   K ;

! +Refl+Pl1+Nom:е          K ;
  +Refl+Pl1+Gen:ӹмнӓ%>н    K ;
  +Refl+Pl1+Acc:ӹмнӓ%>м    K ;
  +Refl+Pl1+Dat:ӹ%>лӓннӓ   K ;

! +Refl+Pl2+Nom:е          K ;
  +Refl+Pl2+Gen:ӹмдӓ%>н    K ;
  +Refl+Pl2+Acc:ӹмдӓ%>м    K ;
  +Refl+Pl2+Dat:ӹ%>лӓндӓ   K ;

! +Refl+Pl3+Nom:е          K ;
  +Refl+Pl3+Gen:ӹмӹшт%>н   K ;
  +Refl+Pl3+Acc:ӹмӹшт%>м   K ;
  +Refl+Pl3+Dat:ӹ%>лӓнӹштӹ K ;

LEXICON DEM-SG !!= @CODE@ тидӹ:ти
     +Dem+Dep:             WORDEND ;
  +Dem+Sg+Nom:дӹ           K       ;
  +Dem+Sg+Gen:дӹ%>н        K       ;
  +Dem+Sg+Acc:дӹ%>м        K       ;
  +Dem+Sg+Dat:дӹ%>лӓн      K       ;
 +Dem+Sg+Cmpr:дӹ%>лӓ       K       ;
  +Dem+Sg+Com:дӹ%>ге       K       ;

  +Dem+Pl+Nom:дӹ%>влӓ      K       ;
  +Dem+Pl+Gen:дӹ%>влӓ%>н   K       ;
  +Dem+Pl+Acc:дӹ%>влӓ%>м   K       ;
  +Dem+Pl+Dat:дӹ%>влӓ%>лӓн K       ;
 +Dem+Pl+Cmpr:дӹ%>влӓ%>лӓ  K       ;
  +Dem+Pl+Com:дӹ%>влӓ%>ге  K       ;

LEXICON DEM-PL !!= @CODE@ нинӹ:ни
        +Dem+Dep:             WORDEND ;
     +Dem+Pl+Nom:нӹ           K       ;
     +Dem+Pl+Gen:нӹ%>н        K       ;
     +Dem+Pl+Acc:нӹ%>м        K       ;
     +Dem+Pl+Dat:нӹ%>лӓн      K       ;
    +Dem+Pl+Cmpr:нӹ%>лӓ       K       ;
     +Dem+Pl+Com:нӹ%>ге       K       ;
!! Plural pronoun with additional plural marking
  +Dem+Pl+Pl+Nom:нӹ%>влӓ      K       ;
  +Dem+Pl+Pl+Gen:нӹ%>влӓ%>н   K       ;
  +Dem+Pl+Pl+Acc:нӹ%>влӓ%>м   K       ;
  +Dem+Pl+Pl+Dat:нӹ%>влӓ%>лӓн K       ;
 +Dem+Pl+Pl+Cmpr:нӹ%>влӓ%>лӓ  K       ;
  +Dem+Pl+Pl+Com:нӹ%>влӓ%>ге  K       ;


LEXICON DemTag
  +Dem+Adn: WORDEND ;
 +Dem: Dem-Cx  ;
!! What are these тӹдӹмӓт, тӹдӹлӓнӓт

LEXICON Dem-Cx
 +Sg+Nom:                K ;
 +Sg+Gen:%>н             K ;
 +Sg+Acc:%>м             K ;
 +Sg+Dat:%>л%{аӓ%}н      K ;

 +Pl+Nom:%>влӓ           K ;
 +Pl+Gen:%>влӓ%>н        K ;
 +Pl+Acc:%>влӓ%>м        K ;
 +Pl+Dat:%>влӓ%>л%{аӓ%}н K ;





! vim: set ft=xfst-lexc:

